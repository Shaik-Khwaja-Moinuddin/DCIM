<properties>
  <properties class="linked-hash-map">
    <entry>
      <string>connectionProperties</string>
      <table>
        <format maxRecords="1" minRecords="1">
          <fields>
            <field name="extended" type="B">
              <defaultValue>0</defaultValue>
            </field>
          </fields>
        </format>
        <records>
          <record/>
        </records>
      </table>
    </entry>
    <entry>
      <string>statisticsProperties</string>
      <table>
        <format maxRecords="2147483647">
          <fields>
            <field description="Channel Name" keyfield="true" name="name" nullable="true" type="S">
              <validators contentType="base64">HEweMSAyMTQ3NDgzNjQ3HRxSHlx3K15eTmFtZSBtYXkgY29udGFpbiBvbmx5IExhdGluIGxldHRlcnMsIGRpZ2l0cywgYW5kIHVuZGVyc2NvcmVzHQ==</validators>
              <help>This is a unique channel ID used to refer to channel data.</help>
              <defaultValue>
                <nullValue/>
              </defaultValue>
            </field>
            <field description="Variable" extendableSelectionValues="true" icon="fi_variable" name="variable" type="S">
              <help>Name of the variable whose value will be used in the statistical channel.</help>
              <defaultValue/>
            </field>
            <field description="Enabled" name="enabled" type="B">
              <help>Controls whether the channel is active and collects data. Default channels should be disabled instead of deleting them in order to prevent their recreation.</help>
              <defaultValue>1</defaultValue>
            </field>
            <field description="Parameters" icon="var_statistics" name="parameters" nullable="true" type="T">
              <help>These are parameters of statistical data collection, storage and processing.</help>
              <defaultValue>
                <table>
                  <format maxRecords="1" minRecords="1">
                    <fields>
                      <field description="Expression" editor="expression" name="expression" type="S">
                        <defaultValue/>
                      </field>
                      <field description="Use Key Field" name="useKeyField" type="B">
                        <defaultValue>0</defaultValue>
                      </field>
                      <field description="Key Field" name="keyField" nullable="true" type="S">
                        <help>Autodetect if value is NULL</help>
                        <defaultValue>
                          <nullValue/>
                        </defaultValue>
                      </field>
                      <field description="NULL Value Processing" name="nullValuesProcessing" type="I">
                        <selectionValues>
                          <option description="Discard">1</option>
                          <option description="Treat As Zero">2</option>
                        </selectionValues>
                        <defaultValue>1</defaultValue>
                      </field>
                      <field description="Out Of Range Value Processing" name="outOfRangeValuesHandling" type="I">
                        <selectionValues>
                          <option description="Do Nothing">0</option>
                          <option description="Discard">1</option>
                          <option description="Normalize">3</option>
                        </selectionValues>
                        <defaultValue>0</defaultValue>
                      </field>
                      <field description="Minimum Value" name="minValue" type="E">
                        <defaultValue>0.0</defaultValue>
                      </field>
                      <field description="Maximum Value" name="maxValue" type="E">
                        <defaultValue>1.0</defaultValue>
                      </field>
                      <field description="Storage" name="storage" type="I">
                        <selectionValues>
                          <option description="Standard file (Low memory usage, slow)">0</option>
                          <option description="Mapped file (High memory usage, fast)">1</option>
                          <option description="Memory (Non-persistent)">2</option>
                          <option description="NoSQL Storage">3</option>
                        </selectionValues>
                        <defaultValue>0</defaultValue>
                      </field>
                      <field description="Type" name="type" type="S">
                        <selectionValues>
                          <option description="Gauge">GAUGE</option>
                          <option description="Counter">COUNTER</option>
                          <option description="Derive">DERIVE</option>
                          <option description="Absolute">ABSOLUTE</option>
                        </selectionValues>
                        <defaultValue>GAUGE</defaultValue>
                      </field>
                      <field description="Average" name="average" type="B">
                        <group>Aggregation</group>
                        <defaultValue>1</defaultValue>
                      </field>
                      <field description="Minimum" name="minimum" type="B">
                        <group>Aggregation</group>
                        <defaultValue>1</defaultValue>
                      </field>
                      <field description="Maximum" name="maximum" type="B">
                        <group>Aggregation</group>
                        <defaultValue>1</defaultValue>
                      </field>
                      <field description="Total" name="total" type="B">
                        <group>Aggregation</group>
                        <defaultValue>0</defaultValue>
                      </field>
                      <field description="First" name="first" type="B">
                        <group>Aggregation</group>
                        <defaultValue>0</defaultValue>
                      </field>
                      <field description="Last" name="last" type="B">
                        <group>Aggregation</group>
                        <defaultValue>0</defaultValue>
                      </field>
                      <field description="Minutely" editor="period" name="minutelyStats" nullable="true" type="L">
                        <editorOptions>2 3</editorOptions>
                        <validators contentType="base64">HEweMTIwMDAwIDkyMjMzNzIwMzY4NTQ3NzU4MDcd</validators>
                        <group>Storage Periods</group>
                        <defaultValue>3600000</defaultValue>
                      </field>
                      <field description="Hourly" editor="period" name="hourlyStats" nullable="true" type="L">
                        <editorOptions>3 4</editorOptions>
                        <validators contentType="base64">HEweNzIwMDAwMCA5MjIzMzcyMDM2ODU0Nzc1ODA3HQ==</validators>
                        <group>Storage Periods</group>
                        <defaultValue>86400000</defaultValue>
                      </field>
                      <field description="Daily" editor="period" name="dailyStats" nullable="true" type="L">
                        <editorOptions>4 6</editorOptions>
                        <validators contentType="base64">HEweMTcyODAwMDAwIDkyMjMzNzIwMzY4NTQ3NzU4MDcd</validators>
                        <group>Storage Periods</group>
                        <defaultValue>2592000000</defaultValue>
                      </field>
                      <field description="Weekly" editor="period" name="weeklyStats" nullable="true" type="L">
                        <editorOptions>5 8</editorOptions>
                        <validators contentType="base64">HEweMTIwOTYwMDAwMCA5MjIzMzcyMDM2ODU0Nzc1ODA3HQ==</validators>
                        <group>Storage Periods</group>
                        <defaultValue>7776000000</defaultValue>
                      </field>
                      <field description="Monthly" editor="period" name="monthlyStats" nullable="true" type="L">
                        <editorOptions>6 8</editorOptions>
                        <validators contentType="base64">HEweNTE4NDAwMDAwMCA5MjIzMzcyMDM2ODU0Nzc1ODA3HQ==</validators>
                        <group>Storage Periods</group>
                        <defaultValue>31536000000</defaultValue>
                      </field>
                      <field description="Yearly" editor="period" name="yearlyStats" nullable="true" type="L">
                        <editorOptions>8 8</editorOptions>
                        <validators contentType="base64">HEweNjMwNzIwMDAwMDAgOTIyMzM3MjAzNjg1NDc3NTgwNx0=</validators>
                        <group>Storage Periods</group>
                        <defaultValue>315360000000</defaultValue>
                      </field>
                      <field description="Timestamp Expression" editor="expression" name="timestampExpression" nullable="true" type="S">
                        <help>Use this expression to override a default timestamp for each new variable value. The expression should evaluate to Date type.</help>
                        <group>Advanced Properties</group>
                        <defaultValue>{#timestamp}</defaultValue>
                      </field>
                      <field description="Protect Statistics" name="protectStatistics" type="B">
                        <help>Disable parameters that change resets statistics data</help>
                        <group>Advanced Properties</group>
                        <defaultValue>1</defaultValue>
                      </field>
                      <field description="Step" editor="period" name="step" type="L">
                        <editorOptions>1 1</editorOptions>
                        <validators contentType="base64">HEweMSA5MjIzMzcyMDM2ODU0Nzc1ODA3HQ==</validators>
                        <help>Changing this value resets statistics data</help>
                        <group>Advanced Properties</group>
                        <defaultValue>60000</defaultValue>
                      </field>
                      <field description="XFiles Factor" name="xff" type="F">
                        <validators contentType="base64">HEweMC4wIDAuOTk5HQ==</validators>
                        <help>Changing this value resets statistics data</help>
                        <group>Advanced Properties</group>
                        <defaultValue>0.9</defaultValue>
                      </field>
                      <field description="Heartbeat" editor="period" name="heartbeat" type="L">
                        <validators contentType="base64">HEweMCA5MjIzMzcyMDM2ODU0Nzc1ODA3HQ==</validators>
                        <help>The maximum time period that may pass between two updates of this data source before the value of the data source is assumed to be &apos;unknown&apos;. &apos;0&apos; means automatic heartbeat.</help>
                        <group>Advanced Properties</group>
                        <defaultValue>0</defaultValue>
                      </field>
                    </fields>
                    <bindings>
                      <binding>
                        <reference>expression#options</reference>
                        <expression>hasResolver(&apos;parent&apos;) ? expressionEditorOptions({.:}, {parent/variable}, 1) : null</expression>
                      </binding>
                      <binding>
                        <reference>keyField#hidden</reference>
                        <expression>!{useKeyField}</expression>
                      </binding>
                      <binding>
                        <reference>minValue#hidden</reference>
                        <expression>{outOfRangeValuesHandling} == 0</expression>
                      </binding>
                      <binding>
                        <reference>maxValue#hidden</reference>
                        <expression>{outOfRangeValuesHandling} == 0</expression>
                      </binding>
                      <binding>
                        <reference>step#enabled</reference>
                        <expression>!{protectStatistics}</expression>
                      </binding>
                      <binding>
                        <reference>xff#enabled</reference>
                        <expression>!{protectStatistics}</expression>
                      </binding>
                    </bindings>
                    <validators contentType="base64">HEUeZTIxcGJsWmhiSFZsZlNBOElIdHRZWGhXWVd4MVpYMGdQeUJ1ZFd4c0lEb2dKMDFwYm1sdGRXMGdWbUZzZFdVZ2MyaHZkV3hrSUdKbA0KSUd4bGMzTmxjaUIwYUdGdUlFMWhlR2x0ZFcwZ1ZtRnNkV1VuDQod</validators>
                  </format>
                  <records>
                    <record>
                      <value name="timestampExpression">
                        <nullValue/>
                      </value>
                    </record>
                  </records>
                </table>
              </defaultValue>
            </field>
          </fields>
          <bindings>
            <binding>
              <reference>variable#choices</reference>
              <expression>{utilities:variablesByMask({.:})}</expression>
            </binding>
          </bindings>
          <validators contentType="base64">HEseHQ==</validators>
        </format>
      </table>
    </entry>
    <entry>
      <string>ccmConfigurationManagement</string>
      <table>
        <format maxRecords="1" minRecords="1">
          <fields>
            <field description="Enabled" name="ccmEnabled" type="B">
              <defaultValue>0</defaultValue>
            </field>
            <field description="Configuration Backup Expression" editor="expression" name="ccmExpressionBackup" type="S">
              <defaultValue>{readConfiguration({configurationType})}</defaultValue>
            </field>
            <field description="Configuration Restore Expression" editor="expression" name="ccmExpressionRestore" type="S">
              <defaultValue>{writeConfiguration({env/sequenceType}, {env/configurationType}, {env/configuration}, {env/restoreParameters})}</defaultValue>
            </field>
            <field description="Maximum Number of Saved Configurations" name="ccmMaxConfigurationsNumber" type="I">
              <validators contentType="base64">HEweMCAyMTQ3NDgzNjQ3HQ==</validators>
              <defaultValue>8</defaultValue>
            </field>
            <field description="Backup Sequence" name="ccmBackupSequence" type="S">
              <defaultValue/>
            </field>
            <field description="Backup Parameters" name="ccmBackupParameters" type="T">
              <defaultValue>
                <table>
                  <format maxRecords="2147483647"/>
                </table>
              </defaultValue>
            </field>
            <field description="Restore Sequence" name="ccmRestoreSequence" type="S">
              <defaultValue/>
            </field>
            <field description="Restore Parameters" name="ccmRestoreParameters" type="T">
              <defaultValue>
                <table>
                  <format maxRecords="2147483647"/>
                </table>
              </defaultValue>
            </field>
            <field description="Comparison Filter" name="ccmComparisonFilter" type="S">
              <help>Regular expression for data that should be excluded from comparison of configurations.</help>
              <defaultValue/>
            </field>
          </fields>
          <bindings>
            <binding>
              <reference>ccmBackupSequence#choices</reference>
              <expression>hasField({plugins.com_tibbo_linkserver_plugin_context_ccm:ccmConfiguration},&quot;name&quot;) ? {utilities:selectionValues(&quot;{plugins.com_tibbo_linkserver_plugin_context_ccm:ccmConfiguration}&quot;, &quot;{name}&quot;,&quot;{name}&quot;)} : null</expression>
            </binding>
            <binding>
              <reference>ccmRestoreSequence#choices</reference>
              <expression>hasField({plugins.com_tibbo_linkserver_plugin_context_ccm:ccmConfiguration},&quot;name&quot;) ? {utilities:selectionValues(&quot;{plugins.com_tibbo_linkserver_plugin_context_ccm:ccmConfiguration}&quot;, &quot;{name}&quot;,&quot;{name}&quot;)} : null</expression>
            </binding>
            <binding>
              <reference>ccmBackupParameters</reference>
              <expression>{plugins.com_tibbo_linkserver_plugin_context_ccm:createDataTableByScriptName({ccmBackupSequence},&quot;backupParametersFormat&quot;,{ccmBackupParameters})}</expression>
            </binding>
            <binding>
              <reference>ccmRestoreParameters</reference>
              <expression>{plugins.com_tibbo_linkserver_plugin_context_ccm:createDataTableByScriptName({ccmRestoreSequence},&quot;restoreParametersFormat&quot;,{ccmRestoreParameters})}</expression>
            </binding>
          </bindings>
        </format>
        <records>
          <record>
            <value name="ccmBackupSequence">Cisco</value>
            <value name="ccmBackupParameters">
              <table>
                <format maxRecords="1" minRecords="1">
                  <fields>
                    <field description="Configuration Type" extendableSelectionValues="true" name="configurationType" type="S">
                      <selectionValues>
                        <option description="Running">Running</option>
                        <option description="Startup">Startup</option>
                      </selectionValues>
                      <defaultValue>Running</defaultValue>
                    </field>
                    <field description="Control Protocol" extendableSelectionValues="true" name="controlProtocol" type="S">
                      <selectionValues>
                        <option description="SNMP">SNMP</option>
                        <option description="SSH">SSH</option>
                        <option description="Telnet">Telnet</option>
                      </selectionValues>
                      <defaultValue>Telnet</defaultValue>
                    </field>
                    <field description="Transport Protocol" extendableSelectionValues="true" name="transportProtocol" type="S">
                      <selectionValues>
                        <option description="SCP">SCP</option>
                        <option description="Shell">Shell</option>
                        <option description="FTP">FTP</option>
                        <option description="TFTP">TFTP</option>
                      </selectionValues>
                      <defaultValue>Shell</defaultValue>
                    </field>
                    <field description="Telnet Port" name="telnetPort" type="I">
                      <defaultValue>23</defaultValue>
                    </field>
                    <field description="SSH Port" name="sshPort" type="I">
                      <defaultValue>22</defaultValue>
                    </field>
                    <field description="Connection Timeout" name="connectionTimeout" type="I">
                      <defaultValue>30000</defaultValue>
                    </field>
                    <field description="Read Timeout" name="readTimeout" type="I">
                      <defaultValue>5000</defaultValue>
                    </field>
                    <field description="User Name" name="username" type="S">
                      <defaultValue/>
                    </field>
                    <field description="Password" editor="password" name="password" type="S">
                      <defaultValue/>
                    </field>
                    <field description="Privileged Password" editor="password" name="privilegedPassword" type="S">
                      <defaultValue/>
                    </field>
                    <field description="SSH Username" name="sshUsername" type="S">
                      <defaultValue/>
                    </field>
                    <field description="SSH Password" editor="password" name="sshPassword" type="S">
                      <defaultValue/>
                    </field>
                    <field description="File Name" name="filename" type="S">
                      <defaultValue/>
                    </field>
                    <field description="End of Line Delimiter" name="eolDelimiter" type="S">
                      <selectionValues>
                        <option description="LF">\n</option>
                        <option description="CR+LF">\r\n</option>
                      </selectionValues>
                      <help>Use &apos;CR+LF&apos; value for &apos;SSH&apos; Control Protocol and &apos;LF&apos; for others protocols.</help>
                      <defaultValue>\r\n</defaultValue>
                    </field>
                  </fields>
                  <bindings>
                    <binding>
                      <reference>eolDelimiter</reference>
                      <expression>({username} == &quot;&quot;) ? (({controlProtocol} != &quot;SSH&quot;) ? &quot;\\n&quot; : &quot;\\r\\n&quot; ) : ({eolDelimiter})</expression>
                    </binding>
                    <binding>
                      <reference>sshUsername#hidden</reference>
                      <expression>({controlProtocol} != &quot;SSH&quot; &amp;&amp; {transportProtocol} != &quot;SCP&quot;)</expression>
                    </binding>
                    <binding>
                      <reference>sshPassword#hidden</reference>
                      <expression>({controlProtocol} != &quot;SSH&quot; &amp;&amp; {transportProtocol} != &quot;SCP&quot;)</expression>
                    </binding>
                    <binding>
                      <reference>sshPort#hidden</reference>
                      <expression>({controlProtocol} != &quot;SSH&quot; &amp;&amp; {transportProtocol} != &quot;SCP&quot;)</expression>
                    </binding>
                    <binding>
                      <reference>telnetPort#hidden</reference>
                      <expression>({controlProtocol} != &quot;Telnet&quot;)</expression>
                    </binding>
                  </bindings>
                </format>
              </table>
            </value>
            <value name="ccmRestoreSequence">Cisco</value>
            <value name="ccmRestoreParameters">
              <table>
                <format maxRecords="1" minRecords="1">
                  <fields>
                    <field description="Configuration Type" extendableSelectionValues="true" name="configurationType" type="S">
                      <selectionValues>
                        <option description="Running">Running</option>
                        <option description="Startup">Startup</option>
                      </selectionValues>
                      <defaultValue>Running</defaultValue>
                    </field>
                    <field description="Control Protocol" extendableSelectionValues="true" name="controlProtocol" type="S">
                      <selectionValues>
                        <option description="SNMP">SNMP</option>
                        <option description="SSH">SSH</option>
                        <option description="Telnet">Telnet</option>
                      </selectionValues>
                      <defaultValue>Telnet</defaultValue>
                    </field>
                    <field description="Transport Protocol" extendableSelectionValues="true" name="transportProtocol" type="S">
                      <selectionValues>
                        <option description="SCP">SCP</option>
                        <option description="Shell">Shell</option>
                        <option description="FTP">FTP</option>
                        <option description="TFTP">TFTP</option>
                      </selectionValues>
                      <defaultValue>Shell</defaultValue>
                    </field>
                    <field description="Telnet Port" name="telnetPort" type="I">
                      <defaultValue>23</defaultValue>
                    </field>
                    <field description="SSH Port" name="sshPort" type="I">
                      <defaultValue>22</defaultValue>
                    </field>
                    <field description="Connection Timeout" name="connectionTimeout" type="I">
                      <defaultValue>30000</defaultValue>
                    </field>
                    <field description="Read Timeout" name="readTimeout" type="I">
                      <defaultValue>5000</defaultValue>
                    </field>
                    <field description="User Name" name="username" type="S">
                      <defaultValue/>
                    </field>
                    <field description="Password" editor="password" name="password" type="S">
                      <defaultValue/>
                    </field>
                    <field description="Privileged Password" editor="password" name="privilegedPassword" type="S">
                      <defaultValue/>
                    </field>
                    <field description="SSH Username" name="sshUsername" type="S">
                      <defaultValue/>
                    </field>
                    <field description="SSH Password" editor="password" name="sshPassword" type="S">
                      <defaultValue/>
                    </field>
                    <field description="File Name" name="filename" type="S">
                      <defaultValue/>
                    </field>
                    <field description="End of Line Delimiter" name="eolDelimiter" type="S">
                      <selectionValues>
                        <option description="LF">\n</option>
                        <option description="CR+LF">\r\n</option>
                      </selectionValues>
                      <help>Use &apos;CR+LF&apos; value for &apos;SSH&apos; Control Protocol and &apos;LF&apos; for others protocols.</help>
                      <defaultValue>\r\n</defaultValue>
                    </field>
                  </fields>
                  <bindings>
                    <binding>
                      <reference>eolDelimiter</reference>
                      <expression>({username} == &quot;&quot;) ? (({controlProtocol} != &quot;SSH&quot;) ? &quot;\\n&quot; : &quot;\\r\\n&quot; ) : ({eolDelimiter})</expression>
                    </binding>
                    <binding>
                      <reference>sshUsername#hidden</reference>
                      <expression>({controlProtocol} != &quot;SSH&quot; &amp;&amp; {transportProtocol} != &quot;SCP&quot;)</expression>
                    </binding>
                    <binding>
                      <reference>sshPassword#hidden</reference>
                      <expression>({controlProtocol} != &quot;SSH&quot; &amp;&amp; {transportProtocol} != &quot;SCP&quot;)</expression>
                    </binding>
                    <binding>
                      <reference>sshPort#hidden</reference>
                      <expression>({controlProtocol} != &quot;SSH&quot; &amp;&amp; {transportProtocol} != &quot;SCP&quot;)</expression>
                    </binding>
                    <binding>
                      <reference>telnetPort#hidden</reference>
                      <expression>({controlProtocol} != &quot;Telnet&quot;)</expression>
                    </binding>
                  </bindings>
                </format>
              </table>
            </value>
          </record>
        </records>
      </table>
    </entry>
    <entry>
      <string>performanceTesting</string>
      <table>
        <format maxRecords="1" minRecords="1">
          <fields>
            <field description="Test Variable Count" name="count" type="I">
              <defaultValue>0</defaultValue>
            </field>
            <field description="Asynchronous Update Period" editor="period" name="period" type="L">
              <editorOptions>0 2</editorOptions>
              <defaultValue>0</defaultValue>
            </field>
          </fields>
        </format>
        <records>
          <record/>
        </records>
      </table>
    </entry>
    <entry>
      <string>managedVariables</string>
      <table>
        <format maxRecords="2147483647" unresizable="true">
          <fields>
            <field description="Name" name="name" readonly="true" type="S">
              <defaultValue/>
            </field>
            <field description="Description" name="description" nullable="true" readonly="true" type="S">
              <defaultValue>
                <nullValue/>
              </defaultValue>
            </field>
            <field description="Group" name="group" nullable="true" readonly="true" type="S">
