            <field description="Metadata Reading Mode" name="metadata" type="I">
              <selectionValues>
                <option description="Do not read">0</option>
                <option description="Read definitions of settings, operations and events">1</option>
                <option description="Read all">2</option>
              </selectionValues>
              <help>The metadata reading mode defines whether device variable, function, event and asset definitions are re-read on every synchronization cycle.</help>
              <group>Metadata</group>
              <defaultValue>1</defaultValue>
            </field>
            <field description="Active Entities" name="activeEntities" type="I">
              <selectionValues>
                <option description="All Entities">0</option>
                <option description="Selected Entities">1</option>
              </selectionValues>
              <help>Device entities (variables, functions and events) that the driver will try to make available: either all available entities, or only entities selected in the device account configuration.</help>
              <group>Metadata</group>
              <defaultValue>0</defaultValue>
            </field>
            <field description="Settings Cache Mode" name="cache" type="I">
              <selectionValues>
                <option description="Database (persistent cache)">0</option>
                <option description="Memory (transient cache)">1</option>
              </selectionValues>
              <help>Defined mode of caching device setting values.</help>
              <group>Metadata</group>
              <defaultValue>0</defaultValue>
            </field>
            <field advanced="true" description="Settings Default Quality" name="settingsDefaultQuality" type="I">
              <group>Metadata</group>
              <defaultValue>192</defaultValue>
            </field>
            <field description="Device Events Storage Period" editor="period" name="eventStoragePeriod" nullable="true" type="L">
              <editorOptions>3 8</editorOptions>
              <help>Defines how long device events will be preserved in server database by default. Zero value disables device event persistence. Per-event persistence settings could be adjusted via global Event Processing Rules.</help>
              <group>Events Storage Period</group>
              <defaultValue>604800000</defaultValue>
            </field>
            <field advanced="true" description="Device Dependency Expression" editor="expression" name="dependency" nullable="true" type="S">
              <help>This expression is evaluated before every synchronization. If evaluation result is FALSE, synchronization is not performed.</help>
              <group>Synchronization</group>
              <defaultValue>
                <nullValue/>
              </defaultValue>
            </field>
            <field advanced="true" description="Status Expression" editor="expression" name="status" nullable="true" type="S">
              <help>This expression is evaluated after every synchronization. It should return textual human-readable description of current device status.</help>
              <group>Synchronization</group>
              <defaultValue>
                <nullValue/>
              </defaultValue>
            </field>
            <field advanced="true" description="Color Expression" editor="expression" name="color" nullable="true" type="S">
              <help>This expression is evaluated after every synchronization. It should return a color indicating current device status.</help>
              <group>Synchronization</group>
              <defaultValue>
                <nullValue/>
              </defaultValue>
            </field>
            <field advanced="true" description="Synchronization Queue Length" name="syncQueueLength" type="I">
              <group>Synchronization</group>
              <defaultValue>500000</defaultValue>
            </field>
            <field description="Latitude Expression" editor="expression" name="latitude" nullable="true" type="S">
              <help>This expression is evaluated after every synchronization. It should return current geographical latitude of the device in the floating point format.</help>
              <group>Location</group>
              <defaultValue>
                <nullValue/>
              </defaultValue>
            </field>
            <field description="Longitude Expression" editor="expression" name="longitude" nullable="true" type="S">
              <help>This expression is evaluated after every synchronization. It should return current geographical longitude of the device in the floating point format.</help>
              <group>Location</group>
              <defaultValue>
                <nullValue/>
              </defaultValue>
            </field>
            <field description="Location History Storage Period" editor="period" name="locationStoragePeriod" nullable="true" type="L">
              <editorOptions>3 8</editorOptions>
              <help>Server will auto-delete historical device locations that are older than this storage period.</help>
              <group>Location</group>
              <defaultValue>
                <nullValue/>
              </defaultValue>
            </field>
            <field advanced="true" description="Enable Offline Alert" name="offlineAlert" type="B">
              <help>Flag that controls whether or not a Device Offline alert will be generated if the device stays in disconnected state for some time.</help>
              <defaultValue>1</defaultValue>
            </field>
            <field advanced="true" description="Virtual Device Network" extendableSelectionValues="true" name="virtualNetwork" type="S">
              <selectionValues>
                <option description="None"/>
              </selectionValues>
              <help>Sets up a virtual device network the device belongs to.</help>
              <defaultValue/>
            </field>
          </fields>
          <bindings>
            <binding>
              <reference>dependency#options</reference>
              <expression>expressionEditorOptions({.:})</expression>
            </binding>
            <binding>
              <reference>status#options</reference>
              <expression>expressionEditorOptions({.:})</expression>
            </binding>
            <binding>
              <reference>color#options</reference>
              <expression>expressionEditorOptions({.:})</expression>
            </binding>
            <binding>
              <reference>latitude#options</reference>
              <expression>expressionEditorOptions({.:})</expression>
            </binding>
            <binding>
              <reference>longitude#options</reference>
              <expression>expressionEditorOptions({.:})</expression>
            </binding>
            <binding>
              <reference>managedVariables$#enabled</reference>
